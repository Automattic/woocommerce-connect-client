{
  "name": "woocommerce-services",
  "version": "1.17.0",
  "scripts": {
	"start": "cross-env-shell NODE_ENV=development npm run exec webpack-dev-server",
	"dist": "cross-env-shell NODE_ENV=production npm run exec webpack",
	"eslint": "npm run exec eslint tasks/ client/",
	"test": "npm run eslint",
	"release": "npm run exec node tasks/release.js",
	"i18n": "cross-env-shell NODE_ENV=i18n npm run exec webpack && npm run exec node tasks/i18n",
	"i18njson": "npm run exec node tasks/i18njson",
	"update-deps": "npx rimraf npm-shrinkwrap.json node_modules && npm install && npm shrinkwrap",
	"exec": "cross-env-shell CALYPSO_CLIENT=true DISABLE_FEATURES=code-splitting,wpcom-user-bootstrap PATH=$PATH:./node_modules/wp-calypso/node_modules/.bin NODE_PATH=$NODE_PATH:./node_modules/wp-calypso/node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Automattic/woocommerce-services"
  },
  "license": "GPL-2.0",
  "description": "Connect allthethings",
  "devDependencies": {
    "archiver": "1.3.0",
    "colors": "1.3.2",
    "confirm-simple": "1.0.3",
    "po2json": "0.4.5",
    "shelljs": "0.8.2",
    "xgettext-js": "1.1.0"
  },
  "dependencies": {
    "cross-env": "5.2.0",
    "eslint-loader": "2.1.1",
    "mini-css-extract-plugin": "0.4.4",
    "postcss-url": "8.0.0",
    "sass-loader": "6.0.7",
    "style-loader": "0.23.1",
    "webpack-dev-server": "3.1.10",
    "whatwg-fetch": "2.0.4",
    "wp-calypso": "github:automattic/wp-calypso#5c892f28b886c821c8047f51eec1e2f0000e2971",
    "wrap-loader": "0.2.0"
  }
}
