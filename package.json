{
  "name": "woocommerce-services",
  "version": "1.22.1",
  "scripts": {
    "prepare": "cd wp-calypso && npm ci && cd ..",
    "start": "calypso-build --watch --config='./webpack.config.js'",
    "build": "calypso-build --config='./webpack.config.js'",
    "dist": "cross-env NODE_ENV=production DISABLE_FEATURES=code-splitting,wpcom-user-bootstrap CALYPSO_CLIENT=true webpack",
    "eslint": "eslint tasks/ client/",
    "test": "npm run eslint && npm run test-client",
    "test-client": "jest -c=tests/client/jest.config.js",
    "test-client:watch": "npm run -s test-client -- --watch",
    "prerelease": "npm run i18n && npm run i18njson",
    "release": "node tasks/release.js",
    "i18n": "cross-env NODE_ENV=i18n DISABLE_FEATURES=code-splitting,wpcom-user-bootstrap CALYPSO_CLIENT=true webpack && node tasks/i18n",
    "i18njson": "node tasks/i18njson",
    "update-deps": "rm npm-shrinkwrap.json && rm -rf node_modules && npm install && npm shrinkwrap",
    "woorelease": "echo yes | npm run release && cp release/woocommerce-services.zip ./"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Automattic/woocommerce-services"
  },
  "license": "GPL-2.0",
  "description": "Connect allthethings",
  "devDependencies": {
    "@automattic/calypso-build": "^5.1.0",
    "archiver": "^3.1.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^0.8.0",
    "colors": "^1.4.0",
    "confirm-simple": "^1.0.3",
    "copy-webpack-plugin": "^5.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "file-loader": "^5.0.2",
    "globby": "^10.0.1",
    "jest": "^24.9.0",
    "nock": "^11.7.0",
    "po2json": "^0.4.5",
    "shelljs": "^0.8.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "xgettext-js": "^3.0.0"
  },
  "dependencies": {
    "@automattic/calypso-color-schemes": "^1.0.0",
