{
  "name": "woocommerce-connect-client",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --hot --inline --watch --content-base dist --port 8085",
    "startwin": "set NODE_ENV=development & npm run webpack-dev-server",
    "webpack-dev-server": "webpack-dev-server --hot --inline --watch --content-base dist --port 8085",
    "dist": "NODE_ENV=production ./node_modules/.bin/webpack -p --config webpack.production.config.js",
    "eslint": "./node_modules/.bin/eslint client/",
    "check-shrinkwrap": "./node_modules/.bin/npm-shrinkwrap-check",
    "test": "npm run check-shrinkwrap && ./node_modules/.bin/mocha-webpack --webpack-config webpack.test.config.js 'client/**/test/*.js' && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Automattic/woocommerce-connect-client"
  },
  "license": "GPL-2.0",
  "description": "Connect allthethings",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "5.8.12",
    "babel-core": "5.8.12",
    "babel-eslint": "^5.0.0",
    "babel-loader": "5.3.2",
    "chai": "3.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jsdom": "8.0.4",
    "json-loader": "^0.5.4",
    "mocha": "2.4.5",
    "mocha-webpack": "^0.1.0",
    "node-sass": "^3.4.2",
    "noop-loader": "^1.0.0",
    "npm-shrinkwrap-check": "0.0.9",
    "react-addons-test-utils": "0.14.7",
    "redbox-react": "^1.2.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "tcomb": "2.7.0",
    "tcomb-form": "0.8.1",
    "tcomb-json-schema": "0.2.5",
    "url-loader": "^0.5.7",
    "webpack": "1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.0.0",
    "webpack-notifier": "^1.3.0"
  },
  "dependencies": {
    "debug": "2.2.0",
    "lodash": "4.5.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "wp-calypso": "automattic/wp-calypso"
  }
}
